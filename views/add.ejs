    <h1>Add Item</h1>

    <form>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="inputfirstname">First name</label>
                <input type="text" class="form-control" id="inputEmail4" placeholder="jhon">
            </div>
            <div class="form-group col-md-6">
                <label for="inputlastname">Last name</label>
                <input type="text" class="form-control" id="inputPassword4" placeholder="jhon">
            </div>
        </div>
        <div class="form-group">
            <label for="inputaccount">account</label>
            <input type="text" class="form-control" id="inputAddress" placeholder="@john">
